{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-native"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
